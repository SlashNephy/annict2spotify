{
  "name": "annict2spotify",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "https://github.com/SlashNephy/annict2spotify",
  "scripts": {
    "start": "next start",
    "dev": "concurrently -n dev: 'yarn:dev:*'",
    "dev:next": "next dev",
    "dev:db": "docker-compose up",
    "dev:db:migrate": "prisma migrate dev",
    "build": "next build && next export",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check .",
    "hadolint": "hadolint Dockerfile",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "format:prisma": "prisma format",
    "introspect": "curl -o graphql/schema.graphql https://raw.githubusercontent.com/kiraka/annict-web/main/app/graphql/beta/schema.graphql",
    "generate": "concurrently -n generate: 'yarn:generate:*'",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "generate:prisma": "prisma generate"
  },
  "dependencies": {
    "@kawaiioverflow/arm": "0.92.0",
    "@mantine/core": "4.2.8",
    "@mantine/hooks": "4.2.8",
    "@mantine/next": "4.2.8",
    "@mantine/notifications": "4.2.8",
    "@prisma/client": "3.14.0",
    "cancelable-promise": "4.3.0",
    "date-fns": "2.28.0",
    "graphql-request": "4.3.0",
    "jsdom": "19.0.0",
    "levenshtein-edit-distance": "3.0.0",
    "next": "12.1.6",
    "next-auth": "4.3.4",
    "node-fetch": "3.2.5",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-query": "3.39.1",
    "react-render-if-visible": "2.1.0",
    "spotify-web-api-node": "5.0.2",
    "tabler-icons-react": "1.48.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/import-types-preset": "2.1.18",
    "@graphql-codegen/typescript": "2.4.11",
    "@graphql-codegen/typescript-graphql-request": "4.4.8",
    "@graphql-codegen/typescript-operations": "2.4.0",
    "@iconify/react": "3.2.2",
    "@types/jsdom": "16.2.14",
    "@types/node": "17.0.31",
    "@types/prettier": "2.6.3",
    "@types/react": "18.0.11",
    "@types/react-dom": "18.0.5",
    "@types/spotify-web-api-node": "5.0.7",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "concurrently": "7.2.1",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-css-import-order": "1.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "graphql": "16.5.0",
    "prettier": "2.6.2",
    "prisma": "3.14.0",
    "typescript": "4.7.3"
  },
  "packageManager": "yarn@3.2.1"
}
