{
  "name": "annict2spotify",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "https://github.com/SlashNephy/annict2spotify",
  "scripts": {
    "start": "next start",
    "dev": "concurrently -n dev: 'yarn:dev:*'",
    "dev:next": "next dev",
    "dev:migrate": "prisma migrate dev",
    "build": "next build",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check .",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "format:prisma": "prisma format",
    "introspect": "curl -o graphql/schema.graphql https://raw.githubusercontent.com/kiraka/annict-web/main/app/graphql/beta/schema.graphql",
    "generate": "concurrently -n generate: 'yarn:generate:*'",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "generate:prisma": "prisma generate"
  },
  "dependencies": {
    "@kawaiioverflow/arm": "0.99.0",
    "@mantine/core": "4.2.12",
    "@mantine/hooks": "4.2.12",
    "@mantine/next": "4.2.12",
    "@mantine/notifications": "4.2.12",
    "@prisma/client": "4.1.1",
    "cancelable-promise": "4.3.0",
    "date-fns": "2.29.1",
    "graphql-request": "4.3.0",
    "jsdom": "20.0.0",
    "levenshtein-edit-distance": "3.0.0",
    "next": "12.2.3",
    "next-auth": "4.10.3",
    "node-fetch": "3.2.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "3.39.2",
    "react-render-if-visible": "2.1.0",
    "spotify-web-api-node": "5.0.2",
    "tabler-icons-react": "1.54.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.11.3",
    "@graphql-codegen/import-types-preset": "2.2.2",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-graphql-request": "4.5.2",
    "@graphql-codegen/typescript-operations": "2.5.2",
    "@slashnephy/eslint-config": "0.1.7",
    "@slashnephy/prettier-config": "0.1.1",
    "@types/jsdom": "20.0.0",
    "@types/node": "17.0.31",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/spotify-web-api-node": "5.0.7",
    "concurrently": "7.3.0",
    "eslint": "8.20.0",
    "graphql": "16.5.0",
    "prettier": "2.7.1",
    "prisma": "4.1.1",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@3.2.2",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  }
}
